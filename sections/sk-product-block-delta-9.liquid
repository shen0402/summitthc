<script>

  $("#compare-price").text($("#subscription-compare-price").text());
  $("#sale-price").text($("#subscription-sale-price").text());
  $("#discount-percentage").text($("#subscription-discount-percentage").text());
  $("#solo-price").text($("#subscription-solo-price").text());

  $(".tab-menu-link-subscription.d-9").click(function() {
    if ($("#tab-link-1").hasClass("w--current")) {
      $("#compare-price").text($("#hidden").text());
      $("#sale-price").text($("#single-sale-price").text());
      $("#discount-percentage").text($("#hidden").text());
      $("#solo-price").text($("#single-solo-price").text());
    } else if ($("#tab-link-2").hasClass("w--current")) {
      $("#compare-price").text($("#subscription-compare-price").text());
      $("#sale-price").text($("#subscription-sale-price").text());
      $("#discount-percentage").text($("#subscription-discount-percentage").text());
      $("#solo-price").text($("#subscription-solo-price").text());
    }
  });




