                              quantity-amount="2"
                              sk-type="standard"
                            >
                              <div class="product-option-text">2-Pack</div>
                              <div class="product-option-price-text">
                                <span style="text-decoration: line-through; margin-right:5px;">
                                  {{ compare_at_price | times: 2 | money -}}
