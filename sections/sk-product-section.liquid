                          data-productid="{{ subscription_product.id }}"
                          id="product-select"
                          class="jc-product-select product-form__variants no-js"
                          style="display: none;"
                        >
                          {% for variant in subscription_product.variants %}
                            {% if variant.available %}
                              <option
                                jc-price="{{variant.price | money}}"
                                {% if variant == subscription_product.selected_or_first_available_variant %}
                                  selected="selected"
                                {% endif %}
                                value="{{ variant.id }}"
                              >
                                {{ variant.title }}
                              </option>
                            {% else %}
                              <option jc-price="Sold Out" disabled="disabled">
                                {{ variant.title }} - {{ 'products.product.sold_out' | t }}
                              </option>
                            {% endif %}
                          {% endfor %}
                        </select>

                        <input
                          type="number"
                          class="form-text-field services-ind-quantity w-input"
                          maxlength="256"
                          name="quantity"
                          value="2"
                          min="1"
                          pattern="[0-9]*"
                          id="Quantity-subscription"
                          required=""
                          style="display:none;"
                        >

                        <div class="rc-widget-injection-parent"></div>

                        <div class="form-btn-wrapper-product">
                          <div class="form-bottom-text-product">
                            Do we ship to your state?
                            <a href="#shipping" class="form-bottom-text-product-link {%if product.id == delta_8%} blue {%elsif product.id == delta_9%} orange {%else%} yellow {%endif%}">Find out</a>
                          </div>
                          <input
                            type="submit"
                            value="Add Subscription to Cart"
