            <div>{{product.metafields.stamped.reviews_count}} Reviews</div>
          </a>
        </div>
        


          <style type="text/css">
            .flex-col-outer-product-2 .stamped-badge-starrating {
              margin-right: 6px !important;
              margin-top: 3px !important;
              display: inline-block;
            }
