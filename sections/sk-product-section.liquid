                  data-w-tab="Tab {{ forloop.index }}"
                  class="w-tab-pane {% if forloop.first == true %}w--tab-active{% endif %}"
                >
                  <div class="tab-inner-product">
                    <div class="tab-img-wrapper-product">
                      <div class="tab-img-inner-product">
                        <img
                          src="{{ image | img_url: '1500x' }}"
                          loading="eager"
                          alt="{{product.title}}"
                          class="tab-img-product"
                        >
                      </div>
                    </div>
                  </div>
                </div>
              {% endfor %}
            </div>
          </div>
        </div>
      </div>
      <div class="flex-col-outer-product-2">
        <div class="flex-col-inner-product">
          <div class="flex-content-holder-product">
            <div class="heading-flex-row-product">
              <div class="heading-flex-col-product">
                {% unless sub_heading == blank %}
                  <div class="sub-heading-wrapper-product">
                    <h2 class="section-heading-sm">{{ sub_heading }}</h2>
                  </div>
                {% endunless %}
                <div class="heading-wrapper-product">
                  <h1 class="section-heading-alt">{{product.metafields.product_info.custom_title}}</h1>
                </div>
              </div>

