                                  <span class="product-option-price-text-alt {%if product.id == delta_8%} blue {%elsif product.id == delta_9%} orange {%else%} yellow {%endif%}">(Save 15%)</span>
                                </div>
                                {% comment %}
                                  <div class="product-option-price-text">$84.98 - <span class="product-option-price-text-alt">Save 15%</span></div>
                                {% endcomment %}
                              </div>
                            </div>
                          </div>
                          <div class="product-options-col">
                            <div class="product-options-col-inner">
                              <div class="product-option-block" quantity-amount="3" sk-type="subscription">
                                <div class="product-option-text">3-Pack</div>
                                <div class="product-option-price-text">
                                  <span style="text-decoration: line-through; margin-right:5px;">
                                    {{ compare_at_price | times: 3 | money -}}
