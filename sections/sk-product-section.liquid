                                <span class="product-option-price-text-alt {%if product.id == delta_8 %} blue {%elsif product.id == delta_9 %} orange {%else%} yellow {%endif%}">
                                  (Save 10%)</span
                                >
                              </div>
                            </div>
                          </div>
                        </div>
                        <div class="product-options-col">
                          <div class="product-options-col-inner">
                            <div class="product-option-block" quantity-amount="3" sk-type="standard">
                              <div class="product-option-text">3-Pack</div>
                              <div class="product-option-price-text">
                                <span style="text-decoration: line-through; margin-right:5px;">
                                  {{ compare_at_price | times: 3 | money }}
                                </span>
                                {{ price | times: 3 | minus: three_pack_discount | money }}
                                <br>
                                <span class=" product-option-price-text-alt {%if product.id == delta_8%} blue {%elsif product.id == delta_9%} orange {%else%} yellow {%endif%}"> (Save 15%)</span>
                              </div>
                            </div>
                          </div>
                        </div>
                      </div>

                      <select
                        name="id"
                        data-productid="{{ product.id }}"
                        id="product-select"
                        class="jc-product-select product-form__variants no-js"
                        style="display: none;"
                      >
                        {% for variant in product.variants %}
                          {% if variant.available %}
                            <option
                              jc-price="{{variant.price | money}}"
                              {% if variant == product.selected_or_first_available_variant %}
                                selected="selected"
                              {% endif %}
                              value="{{ variant.id }}"
                            >
                              {{ variant.title }}
                            </option>
                          {% else %}
                            <option jc-price="Sold Out" disabled="disabled">
                              {{ variant.title }} - {{ 'products.product.sold_out' | t }}
                            </option>
                          {% endif %}
                        {% endfor %}
                      </select>

                      <input
                        type="number"
                        class="form-text-field services-ind-quantity w-input"
                        maxlength="256"
                        name="quantity"
                        value="2"
                        min="1"
                        pattern="[0-9]*"
                        id="Quantity"
                        required=""
                        style="display:none;"
                      >

                      <div class="form-btn-wrapper-product">
                        <div class="form-bottom-text-product">
                          Do we ship to your state?
                          <a href="#shipping" class="form-bottom-text-product-link {%if product.id == delta_8%} blue {%elsif product.id == delta_9%} orange {%else%} yellow {%endif%}">Find out</a>
                        </div>
                        <input
                          type="submit"
                          value="{{cart_btn_text}}"
                          data-wait="Please wait..."
                          class="form-btn-product w-button"
                          {{ cart_disabled }}
                        >
                      </div>
                    {% endform %}
                  </div>
