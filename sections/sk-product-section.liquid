            <h2 class="section-heading-sm">{{ sub_heading }}</h2>
          </div>
        {% endunless %}
        <div class="heading-wrapper-product mobile">
          <h1 class="section-heading-alt">{{product.metafields.product_info.custom_title}}</h1>
        </div>

        <div class="stars-mobile">
          <a href="#reviews-link" class="reviews-wrapper">
