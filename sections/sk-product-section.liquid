{%- assign current_variant = product.selected_or_first_available_variant -%}
{%- assign compare_at_price = current_variant.compare_at_price -%}
{%- assign price = current_variant.price -%}

{% assign is_available = current_variant.available %}
{% if is_available == true %}
  {% assign cart_btn_text = 'Add to Cart' %}
  {% assign cart_disabled = '' %}
{% else %}
  {% assign cart_btn_text = 'Sold Out' %}
  {% assign cart_disabled = 'disabled="disabled"' %}
{% endif %}

{% assign delta_9 = 7650180661506 %}
{% assign delta_8 = 7650180137218 %}
{% assign HCC = 7934597071106 %}

{% assign sub_heading = product.metafields.product_info.sub_heading %}
{% assign badge_image = product.metafields.product_info.badge_image %}
{% assign short_description = product.metafields.product_info.short_description %}
{% assign effects_meter_9 = product.metafields.product_info.effects_meter_9 %}
{% assign product_directions = product.metafields.product_info.directions %}
{% assign product_ingredients = product.metafields.product_info.ingredients %}
{% assign product_safety = product.metafields.product_info.safety %}
