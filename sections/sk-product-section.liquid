{% assign activation_icon = product.metafields.product_info.activation_icon %}
{% assign duration_icon = product.metafields.product_info.duration_icon %}
{% assign dose_icon = product.metafields.product_info.dose_icon %}

{% assign subscription_product_meta = product.metafields.product_info.subscription_product %}
