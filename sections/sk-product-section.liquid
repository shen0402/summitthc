                style="display: block;">

              </span>
            {%- endcomment -%}
            <style type="text/css">
              .stamped-badge-stars.stamped-style-color-star.stamped-style-size-star {
                margin-bottom: -5px !important;
              }
              .flex-col-outer-product-2 .stamped-badge-starrating {
                margin-right: 6px !important;
                margin-top: 3px !important;
                display: inline-block;
              }
            </style>
            <!-- Stamped - End Star Rating Badge -->
            <br>

            {% comment %}
              <div class="product-col-reviews-row-page">
                <img src="//cdn.shopify.com/s/files/1/0636/9355/3922/files/ratings-stars-1-min.png" loading="lazy" alt="5 Stars" class="featured-star-img">
                <a href="#" class="featured-col-reviews-heading-link w-inline-block">
                  <div>103 Reviews</div>
                </a>
              </div>
            {% endcomment %}
            {% unless short_description == blank %}
              <div class="description-wrapper-product desktop">
                <div class="body-rte-dark w-richtext">
                  {{ short_description }}
                </div>
              </div>
            {% endunless %}

            <script>
              $(window).on('load', function () {
                console.log('function 1 ran');

                $('#tab-link-1').click(function () {
                  $('#radio-2').removeClass('btn-active');
                  $('#radio-1').addClass('btn-active');
                  console.log('tab1 clicked');
                  return false;
                });
              });
              $(window).on('load', function () {
                console.log('function 2 ran');

                $('#tab-link-2').click(function () {
                  $('#radio-1').removeClass('btn-active');
                  $('#radio-2').addClass('btn-active');
                  console.log('tab2 clicked');
                });
                jQuery('#radio-2').click();
                console.log('Force clicked 2');
              });
            </script>

            <div
              data-current="Tab 1"
              data-easing="ease"
              data-duration-in="1"
              data-duration-out="1"
              class="tabs-subscription w-tabs"
            >
              <div class="tabs-menu-subscription w-tab-menu {% if subscription_product == blank %}tabs-menu-subscription-hide{% endif %}">
                <a
                  data-w-tab="Tab 1"
                  id="tab-link-1"
                  class="tab-menu-link-subscription w-inline-block w-tab-link  {% if subscription_product == blank %}w--current{% endif %}"
                >
                  <div class="subscription-text-wrapper">
                    <div id="radio-1" class="radio-btn-custom-img {%if product.id == delta_8 %} blue {%elsif product.id == delta_9 %} orange {%else%} yellow {%endif%}"></div>
                    <div class="subscrition-tab-text">One-Time Purchase</div>
                  </div>
                </a>

                <a
                  data-w-tab="Tab 2"
                  id="tab-link-2"
                  class="tab-menu-link-subscription w-inline-block w-tab-link {% unless subscription_product == blank %}w--current{% endunless %}"
                >
                  <div class="subscription-text-wrapper">
                    <div id="radio-2" class="radio-btn-custom-img {%if product.id == delta_8 %} blue {%elsif product.id == delta_9 %} orange {%else%} yellow {%endif%}"></div>
                    <div class="subscrition-tab-text">Subscribe & <span style="text-decoration: underline;">Save 15%</span></div>
                  </div>
                </a>
              </div>

              {%- comment -%}
              {%- endcomment -%}
              {% assign regular_price = price %}
              {% assign discount_amount = 0.15 %}
              {% assign dollars_off = regular_price | times: discount_amount %}
              {% assign discount_price = regular_price | minus: dollars_off %}
              

              <div class="w-tab-content">
                <div
                  data-w-tab="Tab 1"
                  class="w-tab-pane {% if subscription_product == blank %}w--tab-active{% endif %}"
                >
                  <div class="form-block-product ">
                    {% form 'product', product, data-productid: product.id %}
                      <div class="product-options-row">
                        <div class="product-options-col">
                          <div class="product-options-col-inner">
                            <div class="product-option-block" quantity-amount="1" sk-type="standard">
                              <div class="product-option-text">1-Pack</div>
                              <div class="product-option-price-text">
                                <span style="text-decoration: line-through; margin-right:5px;">
                                  {%- comment -%}
                                    {{ discount_price | money -}}
                                  {%- endcomment -%}
                                  
                                </span>
                                {{ price | money }}
                              </div>
                            </div>
                          </div>
                        </div>
                        <div class="product-options-col">
                          <div class="product-options-col-inner">
                            <div
