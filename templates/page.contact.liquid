    $(document).ready(function() { 

      var url = window.location.href;

      if ( url == 'https://www.domain.com/pages/contact?contact' ) {
        $('html, body').animate({
        scrollTop: $('#contact').offset().top-106
        }, 1000);
      }

      if ( url == 'https://www.domain.com/pages/contact?contact_posted=true' ) {
        $('html, body').animate({
        scrollTop: $('#contact').offset().top-106
        }, 1000);
      }

    });

</script>
