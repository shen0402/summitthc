<script>
    $(document).ready(function() {
      function navScroll() {
        if ($(window).scrollTop() > 100) {
          $('.fixed-nav-wrapper').addClass('fixed-nav-wrapper-scrolling');
          $('.nav-link').each(function(i, obj) {
            $(this).addClass('nav-link-scrolling');
          });
          $('.nav-dropdown-toggle').each(function(i, obj) {
            $(this).addClass('nav-dropdown-toggle-scrolling');
          });
        } else {
          $('.fixed-nav-wrapper').removeClass('fixed-nav-wrapper-scrolling');
          $('.nav-link').each(function(i, obj) {
            $(this).removeClass('nav-link-scrolling');
          });
          $('.nav-dropdown-toggle').each(function(i, obj) {
            $(this).removeClass('nav-dropdown-toggle-scrolling');
          });
        };
      }
      navScroll();
      $(window).scroll(function() {
        navScroll();
      });
    });
  </script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/jquery-cookie/1.4.1/jquery.cookie.js"></script>
<script type="text/javascript">
    $(document).ready(function() {
      if (!$.cookie('sk_verification_summit')) {
        $('.verification-modal').fadeIn(500);
        $.cookie('sk_verification_summit', true);
      }
      $('#modal-enter').click(function(e) {
        e.preventDefault();
        $('.verification-modal').fadeOut();
      });
      $('#modal-exit').click(function(e) {
        window.location.replace("https://www.google.com/");
      });
      if (!$.cookie('sk_newsletter_summit')) {
        setTimeout(function() {
          $('.newsletter-modal').fadeIn(500);
          $('body').css('overflow', 'hidden');
        }, 2000);
        var date = new Date();
        date.setTime(date.getTime() + 7776000000);
        $.cookie('sk_newsletter_summit', true, {
          expires: date
        });
      }
      $('.newsletter-close-btn').click(function(e) {
        e.preventDefault();
        $('body').css('overflow', 'auto');
        $('.newsletter-modal').fadeOut(500);
        /* $('.newsletter-modal').fadeTo(500, 0, function(){
           $('.newsletter-modal').hide();
         });*/
      });
    });
  </script>
